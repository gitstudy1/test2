[project]
	description = gitstudy
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/gitstudy1
[access "refs/for/refs/*"]
	read = group user/gitstudy1
	push = group user/gitstudy1
	label-Code-Review = -2..+2 group user/gitstudy1
	label-Verified = -1..+1 group user/gitstudy1
	rebase = group user/gitstudy1
	removeReviewer = group user/gitstudy1
	submit = group user/gitstudy1
[access "refs/heads/*"]
	read = group user/gitstudy1
	create = group user/gitstudy1
	push = +force group user/gitstudy1
	pushMerge = group user/gitstudy1
[access "refs/tags/*"]
	push = +force group user/gitstudy1
	pushTag = group user/gitstudy1
	pushSignedTag = group user/gitstudy1
